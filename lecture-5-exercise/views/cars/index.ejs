<!DOCTYPE html>
<html>
  <head>
    <title>Car App</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <a href="/cars/create">Create new car</a>
    <br>
    <br>
    <table>
      <tr>
        <th>Car Name</th>
        <th>Model Name</th>
        <th>Price</th>
      </tr>
      <% cars.forEach(car => { %>
        <tr>
          <td><%= car.car_name %></td>
          <td><%= car.model_name %></td>
          <td><%= car.price %></td>
        </tr>
      <% }) %>
    </table>
  </body>
</html>
